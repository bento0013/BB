<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bsd.cse.model.input">
    <class name="NgProductLineRecord" table="CS_NG_PRODUCT_LINE" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="NG_PRODUCT_LINE_ID" type="long">
            <generator class="sequence">
                <param name="sequence">ng_product_line_seq</param>
            </generator>
        </id>        
        <many-to-one name="productLine" class="com.bsd.cse.model.input.ProductLine" column="PRODUCT_LINE_ID" not-null="true"/>
        <property name="ngAmount" column="NG_AMOUNT" not-null="true"/>        
        <many-to-one name="ngReason" class="com.bsd.cse.model.backoffice.part.NgReason" column="NG_REASON_ID"  not-null="true"/>
        <property name="createdDate" column="CREATED_DATE" />
        <many-to-one class="com.bsd.cse.model.security.UserInfo" name="createdBy" column="CREATED_BY" />
        <property name="updatedDate" column="UPDATED_DATE" />
        <many-to-one class="com.bsd.cse.model.security.UserInfo" name="updatedBy" column="UPDATED_BY" />
        
    </class>
</hibernate-mapping>

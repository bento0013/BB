<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bsd.cse.model.backoffice.measurement">
    <class name="MeasurementToolType" table="CS_MEASUREMENT_TOOL_TYPE" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="MEASUREMENT_TOOL_TYPE_ID" type="long">
            <generator class="sequence">
                <param name="sequence">measurement_tool_type_seq</param>
            </generator>
        </id>
        <property name="name" column="MEASUREMENT_TOOL_TYPE_NAME" not-null="true"/>
        <property name="description" column="MEASUREMENT_TOOL_DESCRIPTION" not-null="false"/>
        <property name="createdDate" column="CREATED_DATE" />
        <many-to-one class="com.bsd.cse.model.security.UserInfo" name="createdBy" column="CREATED_BY" />
        <property name="updatedDate" column="UPDATED_DATE" />
        <many-to-one class="com.bsd.cse.model.security.UserInfo" name="updatedBy" column="UPDATED_BY" />
    </class>
</hibernate-mapping>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bsd.cse.model.security">
    <class name="Team" table="CS_TEAM" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="TEAM_ID" type="long">
            <generator class="assigned"/>                
        </id>
        <property name="teamName" column="TEAM_NAME" not-null="true"/>                       
    </class>
</hibernate-mapping>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bsd.cse.model.backoffice.part">
    <class name="CheckpointUnit" table="CS_CHECKPOINT_UNIT" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="CHECKPOINT_UNIT_ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="name" column="CHECKPOINT_UNIT_NAME" not-null="true"/>
    </class>
</hibernate-mapping>
